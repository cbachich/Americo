<div class="row page-banner">
  <div class="span5">
    <h1><%= page.title %></h1>
    <ul class="banner-list">
      <% page.bullets.each do |bullet| %>
        <li><%= bullet %></li>
      <% end %>
    </ul>
  </div>
  <div class="span4">
    <%= image_tag page.banner_img_url, class: "img-circle" %>
  </div>
</div>
